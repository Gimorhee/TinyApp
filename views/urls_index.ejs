<% include ./partials/_header %>

<body style="font-size:20px; padding:50px;">

<% if (urls) { %>
  <% for (let key in urls) { %>
    <li> <strong><%=key%></strong>: <%= urls[key] %></li>
    <form method="POST" action="/urls/<%= key %>/delete">
      <button type="submit" class="btn btn-dark">Delete</button>
    </form>
  <% } %>
<%} %>
</body>

</html>

